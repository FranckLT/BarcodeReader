<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>BarcodeReader</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>mobileServlet</servlet-name>
    <servlet-class>servlet.mobileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>mobileServlet</servlet-name>
    <url-pattern>/rooms</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>WebsiteServlet</servlet-name>
    <servlet-class>servlet.WebsiteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WebsiteServlet</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddRoomServlet</servlet-name>
    <servlet-class>servlet.AddRoomServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddRoomServlet</servlet-name>
    <url-pattern>/addRoom</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddCategoryServlet</servlet-name>
    <servlet-class>servlet.AddCategoryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddCategoryServlet</servlet-name>
    <url-pattern>/addCategory</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CategoriesPHPServlet</servlet-name>
    <servlet-class>servlet.CategoriesPHPServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CategoriesPHPServlet</servlet-name>
    <url-pattern>/categories</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CodeGeneratorServlet</servlet-name>
    <servlet-class>servlet.CodeGeneratorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CodeGeneratorServlet</servlet-name>
    <url-pattern>/codeGenerator</url-pattern>
  </servlet-mapping>
</web-app>